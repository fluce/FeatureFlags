@model List<string>
@{
    ViewData["Title"] = "Features";
}
<div class="row">
    <div class="col-md-5">
        <h2>Available features stores</h2>
        <ul>
            @foreach (var store in Model)
            {
                <li><a asp-action="StoreIndex" asp-route-store="@store">@store</a></li>
            }
        </ul>
    </div>
    <div class="col-md-7">

    </div>
</div>


@section Scripts {
    <script>
        $(document).ready(function () {
            (function ($) {

            })(jQuery);
        });
    </script>
}
